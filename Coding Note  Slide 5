package myWhile;

public class NoteSlide5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int sum=0;
        int number=2;
        
        do {
        	sum=sum+number;
        	number=number+5;
        	System.out.print("SUM:"+ sum+ " ");
        }while(number<=10);
	}

}
