package myWhile;

public class NoteSlide6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        for(int i=1; i<3; i++) {
        	System.out.print("YAHOO"+ " ");
        	System.out.print("***");
        }
        
    	System.out.println();

        for(int k=1; k<5; k++) {
        	System.out.print("YAHOO"+ " ");

        }
        System.out.print("***");
	}

}
