package myWhile;

public class NoteSlide4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int sum=0;
        int number=1;
        
        while(number<=10) {
        	sum+=number;
            number=number+5;
            System.out.print("SUM:"+ sum+ " ");
        }
        System.out.println("Good Bye");
        
	}
}

